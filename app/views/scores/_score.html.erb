<div id="<%= dom_id score %>">
  <p>
    <strong>Game:</strong>
    <%= score.game_id %>
  </p>

  <p>
    <strong>Player:</strong>
    <%= score.player_id %>
  </p>

  <p>
    <strong>Hole:</strong>
    <%= score.hole_id %>
  </p>

  <p>
    <strong>Strokes:</strong>
    <%= score.strokes %>
  </p>

  <p>
    <strong>Net:</strong>
    <%= score.net %>
  </p>

  <p>
    <strong>Points:</strong>
    <%= score.points %>
  </p>

</div>
